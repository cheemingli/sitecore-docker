    "Variables": {
      "Module-PowershellExtensions:Package.Path": "[parameter('PowerShellExtensionsWdpFullPath')]",
      "Module-PowershellExtensions:Sql.Server": "[parameter('SitecoreDbServer')]",
      "Module-PowershellExtensions:Sql.Module.Database": "mastercore",
      "Module-SXAFramework:Package.Path": "[parameter('SXAFrameworkWdpFullPath')]",
      "Module-SXAFramework:Sql.Server": "[parameter('SitecoreDbServer')]",
      "Module-SXAFramework:Sql.Module.Database": "mastercore",
      "Module-HabitatImages:Package.Path": "[parameter('HabitatImagesWdpFullPath')]",
      "Module-HabitatImages:Sql.Server": "[parameter('SitecoreDbServer')]",
      "Module-HabitatImages:Sql.Module.Database": "master"
    },
    "Includes": {
      "Module-PowershellExtensions": {
        "Source": ".\\Configuration\\Commerce\\Common\\Common.InstallWdpModule.json"
      },
      "Module-SXAFramework": {
        "Source": ".\\Configuration\\Commerce\\Common\\Common.InstallWdpModule.json"
      },
      "Module-HabitatImages": {
        "Source": ".\\Configuration\\Commerce\\Common\\Common.InstallWdpModule.json"
      },
      "SXAStorefront": {
        "Source": ".\\Configuration\\Commerce\\SXAStorefront\\SXAStorefront.Storefront.json"
      }
    }
  }
  